<p>Welcome <%= @user.name %></p>
<h1>Welcome to Code Q&amp;A</h1>

<div class="col-md-6">
  <h4>Questions</h4>
  <%= link_to 'Add Question', new_user_question_path(current_user) %>
  <% @questions.each do |question| %>
    <div class="home-question">

      <%= question.title %>
    </div>
  <% end %>
</div>
